#!/bin/bash
#SBATCH -J pellet
#SBATCH --nodes=2
#SBATCH --ntasks-per-node=32
##SBATCH -n 64
##SBATCH --gres=gpu:4
#SBATCH --time=00:30:00
#SBATCH -p sched_mit_psfc_r8
#SBATCH --mem=128G
#SBATCH -o C1stdout


PARTS=16
$M3DC1_MPIRUN -n $PARTS split_smb analytic-2K.smb part.smb $PARTS
echo $? > mesh_partitioned

touch started

$M3DC1_MPIRUN -n 64 m3dc1_3d -options_file options_bjacobi.type_mumps

touch finished

